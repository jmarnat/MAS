<instance>  
  <presentation name="sampleProblem" maxConstraintArity="2"  
                maximize="false" format="XCSP 2.1_FRODO" />  
 
  <agents nbAgents="4">  
    <agent name="antenna_A" />  
    <agent name="antenna_B" />  
    <agent name="antenna_C" />  
    <agent name="antenna_D" />  
  </agents>  
 
  <domains nbDomains="1">  
    <domain name="three_fq" nbValues="3">100 200 300</domain>  
  </domains>  
 
  <variables nbVariables="4">  
    <variable name="A" domain="three_fq" agent="antenna_A" />  
    <variable name="B" domain="three_fq" agent="antenna_B" />  
    <variable name="C" domain="three_fq" agent="antenna_C" />  
    <variable name="D" domain="three_fq" agent="antenna_D" />  
  </variables>  
 
<!--   <relations nbRelations="1">
  	<relation name="NEQ" arity="2" nbTuples="3" semantics="soft" defaultCost="0">
  		infinity: 100 100 | 200 200 | 300 300
  	</relation>
  </relations>
 -->
  <predicates nbPredicates="1">  
    <predicate name="NEQ">
    	<parameters>int X1 int X2</parameters>
      	<expression><functional>ne(X1,X2)</functional></expression>
    </predicate>  
  </predicates>  
 
  <constraints nbConstraints="4">  
    <constraint name="AB_cst" arity="2" scope="A B" reference="NEQ">
    	<parameters><ne/>[A B]</parameters>
    </constraint> 
    <constraint name="BC_cst" arity="2" scope="B C" reference="NEQ">
    	<parameters><ne/>[B C]</parameters>
    </constraint> 
    <constraint name="CA_cst" arity="2" scope="C A" reference="NEQ">
    	<parameters><ne/>[C A]</parameters>
    </constraint> 
    <constraint name="AD_cst" arity="2" scope="A D" reference="NEQ">
    	<parameters><ne/>[A D]</parameters>
    </constraint> 
  </constraints>  
</instance>  